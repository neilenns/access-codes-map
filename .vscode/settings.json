{
  "cSpell.enabledSchemes": {
    "json": false
  },
  "cSpell.words": [
    "accesscodes",
    "badcasserole",
    "dotenvx",
    "miniflare",
    "nextjs",
    "nvmrc",
    "opennextjs",
    "pnpx",
    "serviceworker",
    "shadcn",
    "Turborepo"
  ],
  "debug.internalConsoleOptions": "neverOpen",
  "eslint.useFlatConfig": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "explorer.fileNesting.enabled": true,
  "fileNestingUpdater.promptOnAutoUpdate": false,
  "files.exclude": {
    "**/.next": true,
    "**/.open-next": true,
    "**/.pnpm-store": true,
    "**/.turbo": true,
    "**/.wrangler": true,
    "**/dist": true,
    "**/next-env.d.ts": true,
    "**/node_modules": true,
    "**/out": true
  },
  "npm.packageManager": "pnpm"
}
